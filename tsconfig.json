{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": ["**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"],
  "compilerOptions": {
    "module": "esnext",
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "strict": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": ["es2020","dom"],
  }
}
